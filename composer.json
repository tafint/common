{
  "name": "chaos/common",
  "description": "The Chaos library",
  "type": "library",
  "keywords": ["ntd1712", "chaos", "common"],
  "homepage": "https://github.com/ntd1712",
  "license": "BSD 3-Clause",
  "authors": [
    {"name": "Dzung Nguyen", "email": "ntd1712@hotmail.com"}
  ],
  "require": {
    "php": ">=5.4",
    "ramsey/uuid": "~3.5",
    "symfony/polyfill": "~1.3",
    "hassankhan/config": "~0.10",
    "league/container": "~2.2",
    "doctrine/orm": "~2.5.6",
    "zendframework/zend-servicemanager": "2.5.1",
    "zendframework/zend-db": "2.5.1",
    "zendframework/zend-http": "2.5.1",
    "zendframework/zend-json": "2.5.1",
    "zendframework/zend-filter": "2.5.1",
    "zendframework/zend-validator": "2.5.1",
    "zendframework/zend-i18n": "2.5.1",
    "zendframework/zend-mail": "2.5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8"
  },
  "autoload": {
    "files": [
      "src/Chaos/defines.php",
      "src/Chaos/functions.php"
    ],
    "psr-4": {
      "Chaos\\": "src/Chaos/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "archive": {
    "exclude": ["!vendor", "tests", "*phpunit.xml", ".travis.yml", "build.xml", "build.properties", "composer.phar"]
  }
}
